@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ex: <https://w3id.org/FAIR-course-UT/2025-2026/group2/data#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <https://schema.org/> .

# --- 0. DCAT Catalog ---
# A container for all metadata records related to this project.
ex:Catalog
    a dcat:Catalog ;
    dct:title "VNL Dataset Expansion Catalog" ;
    dct:description "A catalog containing the metadata record for the VNL Dataset Expansion, created by FAIR Course Group 2." ;
    dct:publisher [
        a foaf:Organization ;
        foaf:name "2025-2026 FAIR Course Group 2, University of Twente"
    ] ;
    # Links the catalog to the dataset and its record
    dcat:dataset ex:Dataset ;
    dcat:record ex:DatasetRecord .

# --- 0b. DCAT Catalog Record ---
# The metadata record itself, which is what describes the dataset.
ex:DatasetRecord
    a dcat:CatalogRecord ;
    dct:title "Metadata Record for VNL Dataset Expansion" ;
    dct:identifier "https://w3id.org/FAIR-course-UT/2025-2026/group2/data/record" ; # Unique identifier for the record
    dct:modified "2025-11-07"^^xsd:date ;
    foaf:primaryTopic ex:Dataset . # Links the record to the actual dataset resource

# --- 1. Dataset Resource Description ---
# The dataset URI from your data file is used as the main identifier.
ex:Dataset
    a dcat:Dataset ;
    dct:title "VNL Dataset Expansion" ;
    dct:description "A collection of statistics and biographical data for professional volleyball players, including attack percentage, block average per set, height, weight, and position. Additionally includes awards won by the players" ;
    dct:identifier "https://w3id.org/FAIR-course-UT/2025-2026/group2/data" ;
    
    # Information inferred from your namespaces/content:
    dct:subject <http://dbpedia.org/ontology/VolleyballPlayer> ;
    dct:language <http://id.loc.gov/vocabulary/iso639-1/en> ;
    dct:created "2025-10-27"^^xsd:date ;
    dct:issued "2025-11-07"^^xsd:date ;
    dct:modified "2025-11-07"^^xsd:date ;

    # --- CREATORS (The Authors) ---
    dct:creator [
        a foaf:Person ;
        foaf:name "Arda Akyazı" ;
        schema:affiliation "University of Twente"
    ] ;
    dct:creator [
        a foaf:Person ;
        foaf:name "Berry Dominguez" ;
        schema:affiliation "University of Twente"
    ] ;
    
    # --- PUBLISHER (The Organization) ---
    dct:publisher [
        a foaf:Organization ;
        foaf:name "2025-2026 FAIR Course Group 2, University of Twente"
    ] ;
    
    # --- CONTACT POINT (One Person for Queries) ---
    dcat:contactPoint [
        a schema:ContactPoint ;
        schema:name "Arda Akyazı" ;
        schema:email "a.akyazi@student.utwente.nl"
    ] ;

    dct:license <https://creativecommons.org/licenses/by/4.0/> ; # Creative Commons Attribution 4.0
    
    # --- 2. Distribution 1: The Dataset File ---
    dcat:distribution [
        a dcat:Distribution ;
        dct:identifier "https://w3id.org/FAIR-course-UT/2025-2026/group2/data/dist/ttl" ;
        dcat:mediaType <http://www.iana.org/assignments/media-types/text/turtle> ;
        dct:format "TTL" ;
        dcat:downloadURL <./turtle_data_of_whole_dataset.ttl> ;
        dct:title "Volleyball Player Dataset (Turtle RDF)" ;
        dcat:byteSize "22812037"^^xsd:integer ;
    ] ;

    # --- 3. Distribution 2: Dataset Schema/Validation Reference (SHACL) ---
    dcat:distribution [
        a dcat:Distribution ;
        dct:identifier "https://w3id.org/FAIR-course-UT/2025-2026/group2/data/dist/shacl" ;
        dcat:mediaType <http://www.iana.org/assignments/media-types/text/turtle> ;
        dct:format "SHACL Shapes (TTL)" ;
        dct:title "Volleyball Dataset SHACL Constraints" ;
        dcat:downloadURL <./shacl.tll> ;
        dcat:byteSize "3048"^^xsd:integer ;
        dct:conformsTo <http://www.w3.org/ns/shacl>
    ] .
