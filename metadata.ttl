        schema:email "a.akyazi@student.utwente.nl"
    ] ;

    dct:license <[TBD: URI of the license, e.g., https://creativecommons.org/licenses/by/4.0/]> ;
    
    # --- 2. Distribution (The data file itself) ---
    dcat:distribution [
        a dcat:Distribution ;
        dcat:mediaType <http://www.iana.org/assignments/media-types/text/turtle> ;
        dct:format "TTL" ;
        dcat:downloadURL <./turtle_data_of_whole_dataset.ttl> ;
        dct:title "Volleyball Player Dataset (Turtle RDF)" ;
        dcat:byteSize "[TBD: Size of turtle_data_of_whole_dataset.ttl in bytes]"^^xsd:integer
    ] ;

    # --- 3. Dataset Schema/Validation Reference ---
    # The SHACL file describes the constraints and structure of the data.
